<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mcboard">
	
	<insert id="newboard" parameterType="mcboard">
	insert into mcreviewboard(seq,title,writer,contents,regdate) 
	values ((SELECT MAX(SEQ)+1 FROM reviewboard), #{title},#{writer},#{contents},sysdate)
	</insert>
		
	<select id="boardList" resultType="mcboard">
	select * from mcreviewboard
	</select>
	
	<select id="boardDetail" resultType="mcboard" parameterType="mcboard">
	select * from mcreviewboard where seq=#{seq}
	</select>
	
	
	
<!-- <![CDATA[XML태그 아니고 이거 문자열이야]]> -->
</mapper>








