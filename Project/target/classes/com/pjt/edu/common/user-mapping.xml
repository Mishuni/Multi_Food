<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="user">
	
	<!-- crud -->
	
	<select id="oneUser" resultType="user" parameterType="String">
		select * from users where id = #{id}
	</select>
	
